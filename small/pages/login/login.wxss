/* pages/login/login.wxss */
.nav{
  width: 100%;
  height: 150rpx;
  background-color: white;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2;
}
.nav-title{
  position: absolute;
  font-size:40rpx;
  left: 80rpx;
  bottom: 20rpx;
  color: black;
}
.blocks{
  width:100%;
  height: 150rpx;
}
.nav-bg{
  height: 150rpx;
}
.nav-return{
  width: 30rpx;
  height: 60rpx;
  padding: 83rpx 25rpx 0 15rpx; 
}
.nav-return-icon{
  width: 100%;
}
.scrollView{
  height:700rpx;
  z-index: 1;
}
.team{
  padding: 30rpx 40rpx 30rpx 0rpx;
  background-color: white;
  display: flex;
}
.team-icon{
  width: 200rpx;
  padding: 0px 25rpx;
}
.team-img{
  width: 100%;
}
.select-title{
  text-align: center;
  font-size:35rpx;
  position: relative;
  height: 47rpx;
}
.select-icon{
  position: absolute;
  bottom: 10rpx;
  width: 0px;
  height: 0px;
  border-color:   #000 transparent;
  border-width: 0rpx 0rpx 15rpx 15rpx;
  border-style: solid;
  margin-left:10rpx;
}
.select-potion{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  top: 0;
  left: 0;
  z-index: 10;
}
.select-potion-main{
  position: absolute;
  width:300rpx;
  padding:30rpx;
  background-color: white; 
  left: 50%;
  transform: translate(-50%, 0);
  top: 230rpx; 
}
.select-potion-find{
  text-align: center;
  font-size: 35rpx;
  padding: 20rpx 0rpx;
}
.team-content-title{
  width:170px;
  display: block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 35rpx;
  padding: 4rpx 0;
}
.team-content-date{
  font-size: 30rpx;
  color: #999;
}
.team-content-map{
  color: #999;
}
.loading{
  height: 50px;
}
.loading-icon{
  height: 100%;
}
.line{
  width: 100%;
  height: 1px;
  background-color: #ccc;
}
.view{
  height: 30px;
}